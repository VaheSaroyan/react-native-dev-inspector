{
  "name": "react-native-dev-inspector",
  "version": "1.1.0",
  "description": "Click on React Native components to jump to source code in your IDE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./metro": {
      "types": "./dist/metro.d.ts",
      "default": "./dist/metro.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "react-native",
    "expo",
    "inspector",
    "devtools",
    "debug",
    "developer-tools"
  ],
  "license": "MIT",
  "author": "VaheSaroyan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaheSaroyan/react-native-dev-inspector.git",
    "directory": "packages/react-native-dev-inspector"
  },
  "bugs": {
    "url": "https://github.com/VaheSaroyan/react-native-dev-inspector/issues"
  },
  "homepage": "https://github.com/VaheSaroyan/react-native-dev-inspector#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.68.0"
  },
  "dependencies": {
    "@rn-dev-inspector/core": "workspace:*",
    "@rn-dev-inspector/metro-plugin": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0"
  }
}
